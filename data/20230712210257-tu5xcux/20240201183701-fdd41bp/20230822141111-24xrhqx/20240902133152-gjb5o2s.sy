				{
					"Type": "NodeCodeBlockCode",
					"Data": "class Person\n{\n    public string name;\n    public int age;\n    public Person[] friends;\n\n    /// \u003csummary\u003e\n    /// 说话的行为\n    /// \u003c/summary\u003e\n    /// \u003cparam name=\"str\"\u003e说话的内容\u003c/param\u003e\n    public void Speak(string str)\n    {\n        WriteLine(\"{0}说{1}\",name, str);\n    }\n\n    /// \u003csummary\u003e\n    /// 判断是否成年\n    /// \u003c/summary\u003e\n    /// \u003creturns\u003e\u003c/returns\u003e\n    public bool IsAdult()\n    {\n        return age \u003e= 18;\n    }\n  \n    /// \u003csummary\u003e\n    /// 添加朋友的方法\n    /// \u003c/summary\u003e\n    /// \u003cparam name=\"p\"\u003e传入的新朋友\u003c/param\u003e\n    public void AppendFriend(Person p)\n    {\n        if (friends == null)\n        {\n            friends = new Person[] { p };\n        }\n        else\n        {\n\t\t\t// 扩容\n            Person[] newfriends = new Person[friends.Length + 1];\n            for (int i = 0; i \u003c friends.Length; i++)\n            {\n                newfriends[i] = friends[i];\n            }\n            newfriends[friends.Length - 1] = p;\n            friends = newfriends;\n        }\n    }\n}\n\ninternal class Program\n{\n    static void Main(string[] args)\n    {\n        Person p = new Person();\n        p.name = \"MrTang\";\n        p.Speak(\"I Love C#\");\n        p.age = 18;\n        if (p.IsAdult())\n        {\n            p.Speak(\"我成年了\");\n        }\n        Person p2 = new Person();\n        p2.name = \"小明\";\n        p2.age = 16;\n\n        p.AppendFriend(p2);\n\n        for (int i = 0; i \u003c p.friends.Length; i++)\n        {\n            WriteLine(p.friends[i].name);\n        }\n    }\n}\n"
