							"Children": [
								{
									"Type": "NodeMathBlockOpenMarker"
								},
								{
									"Type": "NodeMathBlockContent",
									"Data": "M_{s-f} = \\begin{pmatrix} \n    | \u0026 | \u0026 | \u0026 | \\\\\n    X_s \u0026 Y_s \u0026 Z_s \u0026 O_s \\\\\n    | \u0026 | \u0026 | \u0026 | \\\\\n    0 \u0026 0 \u0026 0 \u0026 1 \n  \\end{pmatrix}"
