												},
												{
													"Type": "NodeMathBlockContent",
													"Data": "\\because \\begin{pmatrix}\n  0 \\\\ 0 \\\\ -Far \\\\ 1\n\\end{pmatrix} \\times Far = \\begin{pmatrix}\n  0 \\\\ 0 \\\\ -Far^2 \\\\ Far\n\\end{pmatrix} (映射的点不变) \\\\ \n\\ \\\\\n\\therefore \\begin{pmatrix}\n  Near \u0026 0 \u0026 0 \u0026 0 \\\\\n  0 \u0026 Near \u0026 0 \u0026 0 \\\\ \n  0 \u0026 0 \u0026 a \u0026 b \\\\ \n  0 \u0026 0 \u0026 -1 \u0026 0\n\\end{pmatrix} \\begin{pmatrix}\n  0 \\\\ 0 \\\\ -Far \\\\ 1 \n\\end{pmatrix} = \\begin{pmatrix}\n  0 \\\\ 0 \\\\ -Far^2 \\\\ Far\n\\end{pmatrix}"
												},
												{
													"Type": "NodeMathBlockCloseMarker"
												}
											]
