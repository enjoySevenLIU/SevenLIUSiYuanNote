						},
						{
							"Type": "NodeText",
							"Data": "、"
						},
						{
							"Type": "NodeTextMark",
							"TextMarkType": "a",
							"TextMarkAHref": "https://docs.unity.cn/cn/2021.1/Manual/class-SphereCollider.html",
							"TextMarkTextContent": "球形碰撞体"
						},
						{
							"Type": "NodeText",
							"Data": "和"
						},
						{
							"Type": "NodeTextMark",
							"TextMarkType": "a",
							"TextMarkAHref": "https://docs.unity.cn/cn/2021.1/Manual/class-CapsuleCollider.html",
							"TextMarkTextContent": "胶囊碰撞体"
						},
						{
							"Type": "NodeText",
							"Data": "。在 2D 中，可以使用 "
						},
						{
							"Type": "NodeTextMark",
							"TextMarkType": "a",
							"TextMarkAHref": "https://docs.unity.cn/cn/2021.1/Manual/class-BoxCollider2D.html",
